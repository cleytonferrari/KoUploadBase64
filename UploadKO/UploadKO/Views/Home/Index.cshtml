<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>Upload</title>
    <script src="~/Scripts/jquery-1.8.2.js"></script>
    <script src="~/Scripts/knockout-2.3.0.js"></script>
    <script src="~/Scripts/knockout-2.3.0.debug.js"></script>
    <script src="~/Scripts/ko_file.js"></script>
    <style type="text/css">
        img.thumb {
            max-width: 100px;
            max-height: 100px;
        }
    </style>
</head>
<body>
    <button type="button" data-bind="click: addSlot">Add slot</button>
    <div data-bind="foreach: { data: images, beforeRemove: beforeRemoveSlot }">
        <div>
            <input type="file" accept="image/*" data-bind="file: imageFile, fileObjectURL: imageObjectURL, imageBase64: image, imageType: imageType" />
            
            <div data-bind="if: imageObjectURL">
                <img class="thumb" data-bind="attr: { src: imageObjectURL }" />
            </div>
            <div>Size: <span data-bind="text: fileSize"></span>&nbsp;Kbytes</div>
            <div>First 10 bytes: <span data-bind="text: firstBytes"></span></div>
            <button type="button" data-bind="click: $parent.removeSlot">Remove</button>
            
            <p data-bind="text: imageSrcRetorno"></p>
            <img class="thumb" data-bind="attr: { src: imageSrcRetorno }" />
        </div>
    </div>
    
    

    <script type="text/javascript">
        ko.applyBindings(imageListModel);
    </script>
</body>
</html>